{
    "cSpell.words": [
        "Chakroun",
        "dbaeumer",
        "frameguard",
        "Gruntfuggly",
        "hsts",
        "humao",
        "intellsmi",
        "pino",
        "prestart",
        "rangav"
    ],
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
        "README.md": "LICENSE, *.md",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/logs": true,
        "**/.devcontainer": true,
        ".gitignore": true,
        "README.md": true,
        "*.json": true,
        ".env*": true,
    }
}